<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.addCallController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Pictures/Add%20Call.jpg" />
         </image>
      </ImageView>
      <CheckBox fx:id="police" layoutX="399.0" layoutY="39.0" mnemonicParsing="false" text="Police" textFill="WHITE" AnchorPane.leftAnchor="380.0" AnchorPane.rightAnchor="130.0" AnchorPane.topAnchor="30.0" />
      <CheckBox fx:id="ambulance" layoutX="399.0" layoutY="73.0" mnemonicParsing="false" text="Ambulance" textFill="WHITE" AnchorPane.leftAnchor="380.0" AnchorPane.rightAnchor="130.0" AnchorPane.topAnchor="70.0" />
      <CheckBox fx:id="fireFighters" layoutX="399.0" layoutY="114.0" mnemonicParsing="false" text="Fire Fighters" textFill="WHITE" AnchorPane.leftAnchor="380.0" AnchorPane.rightAnchor="130.0" AnchorPane.topAnchor="110.0" />
      <TextField fx:id="fName" alignment="CENTER" layoutX="32.0" layoutY="66.0" promptText="First Name" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="470.0" AnchorPane.topAnchor="70.0" />
      <TextField fx:id="lName" alignment="CENTER" layoutX="151.0" layoutY="70.0" promptText="Last Name" AnchorPane.leftAnchor="140.0" AnchorPane.rightAnchor="360.0" AnchorPane.topAnchor="70.0" />
      <TextField fx:id="number" layoutX="252.0" layoutY="70.0" promptText="Phone Number" AnchorPane.leftAnchor="250.0" AnchorPane.rightAnchor="250.0" AnchorPane.topAnchor="70.0" />
      <TextArea fx:id="description" layoutX="30.0" layoutY="137.0" prefHeight="200.0" prefWidth="200.0" promptText="Description" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="250.0" AnchorPane.topAnchor="140.0" />
      <Button fx:id="save" layoutX="380.0" layoutY="228.0" mnemonicParsing="false" onAction="#saveCall" text="Save" AnchorPane.leftAnchor="380.0" AnchorPane.rightAnchor="130.0" AnchorPane.topAnchor="220.0" />
      <Button fx:id="back" layoutX="380.0" layoutY="315.0" mnemonicParsing="false" onAction="#goBack" text="Go Back" AnchorPane.leftAnchor="380.0" AnchorPane.rightAnchor="130.0" AnchorPane.topAnchor="315.0" />
   </children>
</AnchorPane>
