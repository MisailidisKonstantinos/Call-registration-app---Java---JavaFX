<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.MainWindowController">
   <children>
      <ImageView fitHeight="405.0" fitWidth="607.0" layoutX="-7.0" opacity="0.8" pickOnBounds="true" preserveRatio="true" smooth="false">
         <image>
            <Image url="@Pictures/Main%20Window.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addCall" layoutX="274.0" layoutY="55.0" mnemonicParsing="false" onAction="#addCall" text="Add a new call" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="70.0" />
      <Button fx:id="showService" layoutX="274.0" layoutY="105.0" mnemonicParsing="false" onAction="#callBasedOnService" text="Show calls based on service" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="120.0" />
      <Button fx:id="showN" layoutX="241.0" layoutY="164.0" mnemonicParsing="false" onAction="#callBasedOnN" prefHeight="25.0" prefWidth="89.0" text="Show calls based on information" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="170.0" />
      <Button fx:id="rem" layoutX="274.0" layoutY="210.0" mnemonicParsing="false" onAction="#removeCall" text="Remove a call" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="220.0" />
      <Button fx:id="ch" layoutX="271.0" layoutY="269.0" mnemonicParsing="false" onAction="#charts" text="Chart" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="270.0" />
   </children>
</AnchorPane>
